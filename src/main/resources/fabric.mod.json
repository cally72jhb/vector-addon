{
  "schemaVersion": 1,
  "id": "vector-addon",
  "version": "${version}",

  "name": "Vector Addon",
  "description": "Vector is a powerful open-source addon for Meteor Client providing some combat and many utility modules.",
  "authors": [
    "cally72jhb"
  ],
  "contact": {

  },

  "license": "GPL-3.0",

  "environment": "client",
  "entrypoints": {
    "meteor": [
      "cally72jhb.addon.VectorAddon"
    ]
  },
  "mixins": [
    "vector-addon.mixins.json"
  ],
  "accessWidener": "vector-addon.accesswidener",
  "custom": {
    "meteor-client:color": "140,245,165"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "~${mc_version}",
    "meteor-client": "*"
  }
}
